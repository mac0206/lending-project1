{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": "..",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*", "../controller/bookController.ts", "../controller/itemController.ts", "../controller/memberController.ts", "../models/Book.ts", "../models/Member.ts"],
  "exclude": ["node_modules", "dist"]
}

